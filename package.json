{
  "name": "@rpii/wdio-html-reporter",
  "version": "0.5.2",
  "description": "A WebdriverIO plugin. Create a basic HTML formatted report",
  "main": "build/reporter.js",
  "scripts": {
    "build:test": "babel test/* -d build-test --config-file ./babel.config.js",
    "build": "babel src/* -d build/ --config-file ./babel.config.js",
    "clean:build": "rimraf ./build",
    "clean:reports": "rimraf ./reports",
    "test": "mocha build-test/**/*.js --require @babel/register --config-file ./babel.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rpii/wdio-html-reporter.git"
  },
  "directories": {
    "src": "./src"
  },
  "keywords": [
    "html",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "wdio-html-reporter",
    "wdio-html-format-reporter"
  ],
  "author": "Adrien Ruiz Gauder <caritsqa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpii/wdio-html-reporter/issues"
  },
  "homepage": "https://github.com/rpii/wdio-html-reporter#readme",
  "dependencies": {
    "@wdio/reporter": "^5.7.2",
    "chai": "^4.2.0",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.0.11",
    "jpeg-js": "^0.3.3",
    "loadash": "^1.0.0",
    "lodash": "^ 4.17.11",
    "mocha": "^5.0.1",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.1",
    "nightmare": "^2.10.0",
    "opn": "^5.5.0",
    "path": "^0.12.7",
    "pngjs": "^3.3.3",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "eslint": "^4.7.2",
    "eslint-plugin-import": "^2.8.0"
  },
  "contributors": [
    "Adrien Ruiz Gauder <caritsqa@gmail.com>",
    "Gisella Fama <gisella.fama@gmail.com>",
    "George Crawford <george.crawford@ft.com>",
    "Christian Bromann <github@christian-bromann.com>",
    "Markus Benning <ich@markusbenning.de>",
    "Rich Peters <16782353+rpii@users.noreply.github.com>"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
